{
  "name": "adrianocahete.dev",
  "version": "1.0.0",
  "description": "Adriano Cahete's Portfolio",
  "author": "Adriano Cahete",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "dev-debug": "cross-env NODE_OPTIONS='--trace-warnings' npm run dev",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=development nuxt start",
    "generate": "nuxt generate",
    "performance": "cross-env LIGHTHOUSE_BADGES_PARAMS=\"--emulated-form-factor desktop\" npx lighthouse-badges --urls https://adrianocahete.dev/ -r -o ./static/performance/ -b flat",
    "perf_single": "cross-env LIGHTHOUSE_BADGES_PARAMS=\"--emulated-form-factor desktop\" npx lighthouse-badges --urls https://adrianocahete.dev/ -s -o ./static/performance/ -b flat",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "engines": {
    "node": "~13.11.0",
    "npm": "~13.11.0"
  },
  "dependencies": {
    "@nuxtjs/device": "^1.2.7",
    "nuxt": "^2.12.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-plugin-nuxt": ">=0.5.2",
    "nuxt-feature-toggle": "^1.0.1",
    "nuxt-payload-extractor": "^1.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2"
  }
}
